{
  "name": "ithacawebpeople.com",
  "version": "1.0.0",
  "description": "App that powers ithacawebpeople.com, Node + express + Vue.js",
  "main": "index.js",
  "scripts": {
    "start": "node ./",
    "dev": "webpack --watch & nodemon ./",
    "build": "NODE_ENV=production npm run build:client && NODE_ENV=production npm run build:server",
    "build:client": "rm -rf ./dist/public && cp -rf src/public dist/public && webpack --progress --hide-modules",
    "build:server": "rm -rf dist/server && cp -rf src/server dist/server && babel ./src/server --out-dir ./dist/server",
    "postinstall": "npm run build && knex migrate:latest",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "8.1.4"
  },
  "keywords": [
    "Ithaca",
    "Web",
    "People",
    "Funkhaus",
    "Meetup",
    "Vue",
    "Node"
  ],
  "author": "John Robson",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.5",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs-extra": "^4.0.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "node-schedule": "^1.2.4",
    "pg": "^7.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "svg-inline-loader": "^0.8.0",
    "velocity-animate": "^1.5.0",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.2",
    "vuex": "^2.4.0",
    "webpack": "^3.5.5",
    "whatwg-fetch": "^2.0.3",
    "yup": "^0.22.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-root-import": "^5.1.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.5.0",
    "nodemon": "^1.11.0",
    "tap": "^10.7.2"
  }
}
